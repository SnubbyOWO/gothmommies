<?xml version="1.0" encoding="UTF-8" ?>
<Module>
<ModulePrefs title="Papa's Taco Mia!" />
<Content type="html"><![CDATA[

<title>Papa's Taco Mia!</title>
<script src="https://unpkg.com/@ruffle-rs/ruffle"></script>
<script>
window.RufflePlayer = window.RufflePlayer || {};
window.RufflePlayer.config = {
"autoplay": "off",
"polyfills": true,
"letterbox": "on",
"backgroundColor": null,
"scale": "showAll",
"allowFullscreen": true,
"forceScale": true,
"forceAlign": true,
"splashScreen": true,
"wmode": "Window",
"unmuteOverlay": "hidden",
"quality": "high" };
window.addEventListener("load", (event) => {
const ruffle = window.RufflePlayer.newest();
const player = ruffle.createPlayer();
const container = document.getElementById("container");
container.appendChild(player);
player.load("https://cdn.jsdelivr.net/gh/bagel-man/eggyolkers@638d286c58b7952af5ee59af75587db454f3b798/games/swf/papastacomia.swf");
player.style.width = "100%";
player.style.height = "100%";
});
</script>
<div id="container">
</div>
<script src="https://cdn.jsdelivr.net/gh/st39/sdk@main/api.js"></script>
<script src="https://cdn.jsdelivr.net/npm/jquery@3.3.1/dist/jquery.min.js"></script>		
		
		
]]></Content>
</Module>
